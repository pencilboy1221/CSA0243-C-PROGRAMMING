#include<stdio.h>
#include<conio.h>
void main()
{
 float a,b,c,s;
 float area;
 clrscr();
 printf("enter the value of a:",a);
 scanf("%f",&a);
 printf("enter the value of b:",b);
 scanf("%f",&b);
 printf("enter the value of c:",c);
 scanf("%f",&c);
 s=((a+b+c)/2);
 area=(sqrt(s*(s-a)*(s-b)*(s-c)));
 printf("area of the triangle is %d",area);
}
